{
  "name": "jonjon1992/s3-php",
  "version":"2.1.0",
  "description": "Uma biblioteca PHP simples e eficiente para trabalhar com Amazon S3",
  "keywords": ["aws", "s3", "amazon", "storage", "php"],
  "license": "MIT",
  "authors": [
    {
      "name": "jonjon1992",
      "email": "jonjon1992@example.com"
    }
  ],
  "require": {
    "php": "^8.0",
    "aws/aws-sdk-php": "^3.350",
    "ext-json": "*",
    "ext-openssl": "*"
  },
  "require-dev": {
    "squizlabs/php_codesniffer": "3.10.3",
    "phpunit/phpunit": "^10.0",
    "phpstan/phpstan": "^1.10",
    "rector/rector": "^0.19",
    "infection/infection": "^0.27",
    "vimeo/psalm": "^5.0"
  },
  "autoload": {
    "psr-4": {
      "S3\\Lib\\": "lib"
    },
    "files": [
      "lib/helper.php"
    ]
  },
  "autoload-dev": {
    "psr-4": {
      "Tests\\": "tests"
    }
  },
  "config": {
    "process-timeout": 0,
    "optimize-autoloader": true,
    "sort-packages": true
  },
  "scripts": {
    "pre-autoload-dump": "Aws\\Script\\Composer\\Composer::removeUnusedServices",
    "test": "phpunit",
    "test-coverage": "phpunit --coverage-html coverage",
    "cs": "phpcs --standard=phpcs.xml",
    "cs-fix": "phpcbf --standard=phpcs.xml",
    "stan": "phpstan analyse",
    "rector": "rector process",
    "infection": "infection",
    "psalm": "psalm",
    "quality": [
      "@cs",
      "@stan",
      "@test"
    ],
    "fix": [
      "@cs-fix",
      "@rector"
    ]
  },
  "extra": {
    "aws/aws-sdk-php": [
      "S3",
      "Sts"
    ]
  },
  "minimum-stability": "stable",
  "prefer-stable": true
}
